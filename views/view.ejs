<% include partials/header %>
        <div class="container">
            <div class="row">
                <div class="col">
                    <h1><%= title %></h1>
                    <h3>Submitted Information</h3>
                </div>
            </div>
            <div class="row">
                <% if(users){ %>
                <div class="col-3">
                    <section class="results">
                        <h3>User # <%= user.id %></h3>
                        <p>Name: <%= user.name %></p>
                        <p>Age: <%= user.age %></p>
                        <a href="delete/<%= user.id%>">Delete this one</a>
                    </section>
                </div>
            </div>
                <%  } else { %>
            <div class="row">
                <div class="col"></div>
                    <p class="error">There was an error.  No form data submitted.</p>
                <% } %>
                    <a href="/" class="btn btn-primary btn-sm" title="Go Back">Go Back</a>
                </div>
            </div>
        </div>
<% include partials/footer %>